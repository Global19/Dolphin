<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.dianping.paas.core.dao.AppDao">

    <select id="findAll" resultType="AppEntity">
        SELECT * FROM app;
    </select>

    <select id="getByAppId" resultType="AppEntity">
        SELECT * FROM app WHERE app_id = #app_id#;
    </select>
</mapper>

